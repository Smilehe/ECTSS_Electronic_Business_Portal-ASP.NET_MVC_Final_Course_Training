@{
    ViewBag.Title = "OrderList";
}
@using Webdiyer.WebControls.Mvc
@model PagedList<Shop.Models.Order>
<link href="~/Content/amazeui.min.css" rel="stylesheet" />
<link href="~/Content/FH/button.min.css" rel="stylesheet" />
<div>
    <div class="svg-wrapper" style="margin-bottom:30px; margin-top:8px; margin-left:10px;">
        <svg height="40" width="150">
            <rect id="shape" height="40" width="150" />
            <div id="text">
                @Html.ActionLink("新增订单", "Create", new { @class = "spot" })
            </div>
        </svg>
    </div>
</div>    
<div id="articles">
    @Html.Partial("_OrderList", Model)
</div>
<script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
<script type="text/javascript">
$(function () {@{ Ajax.LoadMvcPagerScript(); }});
</script>
